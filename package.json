{
  "name": "@styrsomissionskyrka/styrsomissionskyrka.se",
  "version": "0.0.0",
  "main": "index.js",
  "repository": "git@github.com:styrsomissionskyrka/styrsomissionskyrka.se.git",
  "author": "Adam Bergman <adam@fransvilhelm.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "test": "jest",
    "lint": "eslint --ext .js --ext .ts --ext .tsx --cache src gatsby",
    "clean": "gatsby clean && del '**/__generated__' schema.json",
    "schema:download": "echo '' | yarn gatsby repl",
    "schema:types": "apollo client:codegen --target=typescript --includes=./src/**/*.ts,./src/**/*.tsx,./gatsby/**/*.ts",
    "schema:watch": "npx nodemon --ignore '__generated__/**' --watch 'src' -e ts,tsx,js,jsx --exec 'yarn schema:types'",
    "schema": "yarn schema:download && yarn schema:types"
  },
  "dependencies": {
    "@reach/skip-nav": "^0.8.6",
    "classnames": "^2.2.6",
    "date-fns": "^2.10.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "6.0.0-beta"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-typescript": "^7.8.3",
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.5.0",
    "@types/classnames": "^2.2.10",
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@typescript-eslint/eslint-plugin": "2.23.0",
    "@typescript-eslint/parser": "2.23.0",
    "apollo": "^2.25.0",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "10.1.0",
    "babel-preset-gatsby": "^0.2.33",
    "cz-conventional-changelog": "^3.1.0",
    "del-cli": "^3.0.0",
    "dotenv": "^8.2.0",
    "eslint": "6.x",
    "eslint-config-react-app": "5.2.0",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "2.5.0",
    "gatsby": "^2.19.34",
    "gatsby-image": "^2.2.43",
    "gatsby-plugin-extract-schema": "^0.0.5",
    "gatsby-plugin-manifest": "^2.2.45",
    "gatsby-plugin-offline": "^3.0.38",
    "gatsby-plugin-postcss": "^2.1.21",
    "gatsby-plugin-react-helmet": "^3.1.23",
    "gatsby-plugin-sharp": "^2.4.8",
    "gatsby-plugin-typescript": "^2.2.2",
    "gatsby-source-contentful": "^2.1.92",
    "gatsby-source-filesystem": "^2.1.51",
    "gatsby-transformer-sharp": "^2.3.17",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "postcss-import": "^12.0.1",
    "prettier": "^1.19.1",
    "source-map-support": "^0.5.16",
    "tailwindcss": "^1.2.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,md,yml,html}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "proseWrap": "always"
  }
}
