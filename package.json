{
  "name": "@styrsomissionskyrka/styrsomissionskyrka.se",
  "version": "0.0.0",
  "main": "index.js",
  "repository": "git@github.com:styrsomissionskyrka/styrsomissionskyrka.se.git",
  "author": "Adam Bergman <adam@fransvilhelm.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "test": "jest",
    "generate:schema": "echo '\n' > ./repl && yarn gatsby repl < ./repl && rm -rf ./repl",
    "generate:types": "apollo client:codegen --addTypename --target=typescript --localSchemaFile=./schema.json --includes=./src/**/*.tsx",
    "generate:watch": "npx nodemon --ignore '__generated__/**' --watch 'src' -e ts,tsx,js,jsx --exec 'yarn generate:types'",
    "generate": "yarn generate:schema && yarn generate:types"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@fullhuman/postcss-purgecss": "^2.0.6",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.4",
    "@types/react": "^16.9.21",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "apollo": "^2.22.1",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "10.x",
    "cz-conventional-changelog": "^3.1.0",
    "dotenv": "^8.2.0",
    "eslint": "6.x",
    "eslint-config-react-app": "5.1.0",
    "eslint-plugin-flowtype": "3.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "1.x",
    "gatsby": "^2.19.18",
    "gatsby-image": "^2.2.41",
    "gatsby-plugin-extract-schema": "^0.0.5",
    "gatsby-plugin-manifest": "^2.2.41",
    "gatsby-plugin-offline": "^3.0.35",
    "gatsby-plugin-postcss": "^2.1.20",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-plugin-typescript": "^2.1.27",
    "gatsby-source-contentful": "^2.1.86",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-transformer-sharp": "^2.3.14",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "postcss-import": "^12.0.1",
    "prettier": "^1.19.1",
    "tailwindcss": "^1.2.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,md,yml,html}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "proseWrap": "always"
  }
}
